{
    "name": "joy/backpack-api-laravel-demo",
    "description": "A Laravel + Backpack installation to show off most features.",
    "keywords": ["backpack", "laravel", "backpack for laravel", "admin panel", "crud"],
    "license": "proprietary",
    "type": "project",
    "require": {
        "php": ">=7.3",
        "laravel/framework": "8.49.1",
        "fideloper/proxy": "4.4.1",
        "backpack/crud": "4.1.48",
        "backpack/logmanager": "4.0.4",
        "backpack/settings": "3.0.11",
        "backpack/pagemanager": "3.0.9",
        "backpack/menucrud": "2.0.5",
        "backpack/newscrud": "4.0.4",
        "backpack/permissionmanager": "6.0.9",
        "backpack/backupmanager": "3.0.3",
        "spatie/laravel-translatable": "4.6.0",
        "laravel/tinker": "2.6.1",
        "facade/ignition": "2.10.2",
        "intervention/image": "2.5.1",
        "backpack/revise-operation": "1.0.7",
        "backpack/filemanager": "1.1.5",
        "consoletvs/charts": "6.5.5",
        "laravel/legacy-factories": "1.1.0"
    },
    "require-dev": {
        "fakerphp/faker": "1.14.1",
        "mockery/mockery": "1.4.3",
        "phpunit/phpunit" : "9.5.6",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "^5.0.2|3.0.*|2.8.*",
        "laracasts/generators": "2.0.0",
        "backpack/generators": "3.1.9",
        "itsgoingd/clockwork": "5.0.8",
        "barryvdh/laravel-debugbar": "3.6.2",
        "jeroen-g/laravel-packager": "2.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan vendor:publish --provider=\"Backpack\\CRUD\\BackpackServiceProvider\" --tag=public --force"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "test": "vendor/bin/phpunit --testdox"
    },
    "config": {
        "preferred-install": "dist",
        "platform": {
            "php": "7.4.0"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
    }
}
